{
  "name": "filmlab",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "generate:shaders": "tsx scripts/generate-shaders.ts",
    "generate:stock-luts": "python scripts/generate_stock_luts.py",
    "baseline:render": "tsx scripts/render-baseline.ts",
    "dev": "pnpm run generate:shaders && vite",
